{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "~7.4",
    "laravel/lumen-framework": "^6.0",
    "firebase/php-jwt": "^4.0",
    "spatie/laravel-fractal": "^5.0",
    "vlucas/phpdotenv": "^3.3",
    "devster/ubench": "^1.2",
    "pda/pheanstalk": "^3.1",
    "predis/predis": "^1.1",
    "hashids/hashids": "^2.0",
    "barryvdh/laravel-cors": "^1.0",
    "guzzlehttp/guzzle": "^6.3",
    "ext-json": "*",
    "illuminate/redis": "^6.0",
    "appzcoder/lumen-routes-list": "^1.1",
    "barryvdh/laravel-dompdf": "^0.8.6",
    "propaganistas/laravel-phone": "^4.2"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Helpers/CarbonTimestampHelper.php",
      "app/Helpers/LumenHelper.php",
      "app/Helpers/ScorpionPasswordHelper.php",
      "app/Helpers/StringHelpers.php",
      "app/Helpers/PositionHelper.php",
      "app/Helpers/UrlHelpers.php",
      "app/Helpers/UnitHelper.php",
      "app/Helpers/VehicleHelper.php",
      "app/Helpers/AuxProcessor.php",
      "app/Helpers/UnitHelper.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
